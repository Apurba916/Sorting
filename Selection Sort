#include <stdio.h>

int main() {
    int num_arr[10];
    printf("Enter 10 numbers:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &num_arr[i]);
    }
    for (int i = 0;i < 10 - 1;i++) {
        int smallest_index = i;
        for (int j = i + 1; j < 10; j++) {
            if(num_arr[i] > num_arr[j]){
            smallest_index = j;
            int temp;
            temp = num_arr[i];
            num_arr[i] = num_arr[smallest_index];
            num_arr[smallest_index] = temp;
            }
        }
    }
    printf("The sorted array is:\n");
    for (int k = 0; k < 10 ;k++) {
        printf("%d ", num_arr[k]);
    }
    return 0;
}
