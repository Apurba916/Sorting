#include <stdio.h>
int main(){
   int arr[10];
   printf("Enter 10 numbers:\n");
   for (int i = 0; i < 10; i++)
   {
    scanf("%d", &arr[i]);
   }
   
   for (int j = 0; j < 10; j++)
   {
    for (int k = 0; k < 10 - j - 1; k++)
    {
        if (arr[k] > arr[k + 1])
        {
            int temp = arr[k];
            arr[k] = arr[k + 1];
            arr[k +1] = temp;
        }
        else
        {
            continue;
        }
        
    }
    
   }

   printf("The sorted array is:\n");
   for (int l = 0; l < 10; l++)
   {
    printf("%d ", arr[l]);
   }
   
return 0;
}
